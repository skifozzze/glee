          <div class="product__filter filter">
            <div class="filter-category">
              <h4 class="filter__title">CATEGORY</h4>
              <form class="filter-category__form" action="#">
                <label class="filter-category__label">
                  <input class="filter-category__input" type="checkbox" checked>
                  <div class="filter-category__checkbox">
                    <span>Chair</span>
                    <span>></span>
                  </div>
                </label>
                <label class="filter-category__label">
                  <input class="filter-category__input" type="checkbox">
                  <div class="filter-category__checkbox">
                    <span>Table</span>
                    <span>></span>
                  </div>
                </label>
                <label class="filter-category__label">
                  <input class="filter-category__input" type="checkbox">
                  <div class="filter-category__checkbox">
                    <span>Lamp</span>
                    <span>></span>
                  </div>
                </label>
                <label class="filter-category__label">
                  <input class="filter-category__input" type="checkbox">
                  <div class="filter-category__checkbox">
                    <span>Watch</span>
                    <span>></span>
                  </div>
                </label>
                <label class="filter-category__label">
                  <input class="filter-category__input" type="checkbox">
                  <div class="filter-category__checkbox">
                    <span>Clock</span>
                    <span>></span>
                  </div>
                </label>
              </form>
            </div>
            <div class="filter-price">
              <h4 class="filter__title">PRICE</h4>
              <form class="price__form" action="#">
                <input class="price__input" type="text" data-min="0" data-max="1000" data-from="100" data-to="500">
                <label class="price__label">
                  <p class="price__sum">$<span class="price__label-from"></span></p>
                  <p class="price__sum">$<span class="price__label-to"></span></p>
                </label>
              </form>
            </div>
            <div class="filter-brand filter">
              <h4 class="filter__title">BRAND</h4>
              <form class="filter-brand__form" action="#">
                <label class="filter-brand__label">
                  <input class="filter-brand__input" type="checkbox" checked>
                  <div class="filter-brand__checkbox">
                    <span>RFL</span>
                    <span>(10)</span>
                  </div>
                </label>
                <label class="filter-brand__label">
                  <input class="filter-brand__input" type="checkbox">
                  <div class="filter-brand__checkbox">
                    <span>Brothers</span>
                    <span>(21)</span>
                  </div>
                </label>
                <label class="filter-brand__label">
                  <input class="filter-brand__input" type="checkbox">
